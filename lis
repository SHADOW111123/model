#include <iostream>
#include <cstdio>
#define ll long long
#define rg register int
using namespace std;

int n, ans = 0;
int a[10005];
int f[10005];

int main(){
    cin >> n;
    for(rg i = 1; i <= n; ++i) cin >> a[i];

    for(rg i = 1; i <= n; ++i){
        f[i] = 1;  // ✅ 初始为1，不是0
        for(rg j = 1; j < i; ++j){
            if(a[j] < a[i]){
                f[i] = max(f[i], f[j] + 1); 
            }
        }
        ans = max(ans, f[i]); 
    }

    cout << ans << endl;
    return 0;
}

