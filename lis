#include <iostream>
#include <cstdio>
#define ll long long
#define rg register int
using namespace std;

int n, ans = 0;
int a[10005];
int f[10005];

int main(){
    cin >> n;
    for(rg i = 1; i <= n; ++i) cin >> a[i];

    for(rg i = 1; i <= n; ++i){
        f[i] = 1;  // ✅ 初始为1，不是0
        for(rg j = 1; j < i; ++j){
            if(a[j] < a[i]){
                f[i] = max(f[i], f[j] + 1); 
            }
        }
        ans = max(ans, f[i]); 
    }

    cout << ans << endl;
    return 0;
}





//二分lis

    int n;
    cin >> n;
    vector<int> vec(n + 1);
    for1
        cin >> vec[i];

    vector<int> dp(n + 1);下标i结尾的最长lis长度
    vector<int> binary(n + 1);//长度为i的lis的最小尾元素

    fill(dp.begin() + 1, dp.end(),1);
    fill(binary.begin()+1, binary.end(), 0);
    binary[1] = vec[1];

    int ans = 1;
    for1
    {
        auto it = lower_bound(binary.begin() + 1, binary.begin() + ans + 1, vec[i]);

    int index = it - binary.begin();
    binary[index] = vec[i];

    dp[i] = index;
    if (index > ans)
        ans++;

    }





