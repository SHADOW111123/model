#include<iostream>
#include<vector>
#include<stack>
using namespace std;
#define int long long
signed main()
{

	int n;
	cin >> n;
	stack<int> s;
	vector<int> a(n+1);
	vector<int> ans(n+1);
	for (int i = 1; i <= n; i++)
	{
		cin >> a[i];
	}
	for (int i = n; i >= 1; i--)
	{
		while (!s.empty()&&a[i]>=a[s.top()])
		{
			s.pop();
			
		}
		if (!s.empty())
			ans[i] = s.top();
		else
			ans[i] = 0;
		s.push(i);

	}



	for (int i = 1; i <= n; i++)
	{
		cout << ans[i] << " ";
	}


}
//1，每个数字右边的第一个比它大的数的下标，从1开始；


